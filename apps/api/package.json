        "axios": "^1.6.5",
        "bcryptjs": "^2.4.3",
        "bull": "^4.11.5",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "nodemailer": "^7.0.11",
