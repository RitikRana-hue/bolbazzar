        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
